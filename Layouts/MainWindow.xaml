<Window x:Class="Layouts.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Layouts"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="300" MinHeight="160" MinWidth="300">
    <StackPanel>
        <Button Content="StackPanel" Margin="5" Click="Button_Click"></Button>
        <Button Content="Dock" Margin="5" Click="Button_Click"></Button>
        <Button Content="Grid" Margin="5" Click="Button_Click"></Button>
        <Button Content="Grid Splitter" Margin="5" Click="Button_Click"></Button>
    </StackPanel>
</Window>
